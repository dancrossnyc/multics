/* BEGIN INCLUDE FILE:  dl_handler_options.incl.pl1 */

/* This is the switches argument to dl_handler_$switches

   Jay Pattin 7/2/83 */

declare   1 dl_handler_options,
          2 no_question                 bit (1) unaligned,  /* ON = reset switches without querying */
          2 library                     bit (1) unaligned,  /* ON = use installatio_tools_ */
          2 raw                         bit (1) unaligned,  /* ON = use hcs_, not object_type_ (library takes precedence) */
          2 mbz                         bit (33) unaligned;

/* END INCLUDE FILE: dl_handler_options.incl.pl1 */
