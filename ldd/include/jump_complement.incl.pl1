dcl	jump_complement(2:9) bit(9) aligned static options(constant)
	init("001010011"b,	/* jump_true  -> jump_false	*/
	     "001010010"b,	/* jump_false -> jump_true	*/
	     "001011001"b,	/* jump_if_lt -> jump_if_ge	*/
	     "001011000"b,	/* jump_if_gt -> jump_if_le	*/
	     "001010111"b,	/* jump_if_eq -> jump_if_ne	*/
	     "001010110"b,	/* jump_if_ne -> jump_if_eq	*/
	     "001010101"b,	/* jump_if_le -> jump_if_gt	*/
	     "001010100"b);	/* jump_if_ge -> jump_if_lt	*/
