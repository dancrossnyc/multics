/* BEGIN INCLUDE FILE pascal_version_id.incl.pl1 */

/* Identification of current PASCAL implementation */

     dcl	   version	      char (11) int static options (constant) init ("PASCAL 7.02");
     dcl	   version_number	      fixed bin int static options (constant) init (2);
						/* 0 : until PASCAL 5.02
						   1 : for PASCAL 6.xx
						   2 : since PASCAL 7.00 */
     dcl	   gen_id		      char (100) varying init
			      ("Grenoble University Multics Pascal Compiler, Release 7.02, (experimental)");

/* END INCLUDE FILE pascal_version_id.incl.pl1 */
