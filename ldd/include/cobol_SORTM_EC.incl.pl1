
/* BEGIN INCLUDE FILE ...   cobol_SORTM_EC.incl.pl1 */
	/* Structure indexed by SM_$ec to produce error code (Dave Ward 04/13/74). */
dcl	  cobol_sort_$ec		fixed bin(35) ext;
dcl	SORTM_EC		(11) char(52) var static internal init(
			"RELEASE terminated more than once",
			"Record bit length > max1",
			"RELEASE can't open SI SR SS MI",
			"Number of merge strings > max4",
			"MSORT can't open MR.n MS.n",
			"RETURN terminated more than once",
			"RETURN can't set lengths",
			"User requesting more records than input (single)",
			"No records input",
			"RETURN can't truncate SI SR",
			"User requesting more records than input (multiple)"
						);
/* END INCLUDE FILE ...   cobol_SORTM_EC.incl.pl1 */

