
/* BEGIN INCLUDE FILE ...   cobol_SORTM_entries.incl.pl1 */
	/* Declarations for SORTM entries Version 2 (Dave Ward 04/17/74). */
	/* Additions to declarations for SORTM entries Version 2 (G Mercuri 02/28/75). */
dcl	  cobol_sort_$SET_		entry options(variable) ext,	/* 1 or more char(*). */
	  cobol_sort_$CPP		entry (entry,entry) ext,
			/* Note 2-nd parameter of CPP not required. */
	  cobol_sort_$RELEASE_		entry ext,
	  cobol_sort_$RETURN_		entry ext,
    (	  cobol_sort_$sn1,
	  cobol_sort_$sn2,
	  cobol_sort_$fb1,
	  cobol_sort_$fb2,
	  cobol_sort_$bl1,
	  cobol_sort_$bl2,
	  cobol_sort_$result,
	  cobol_sort_$RELbl,
	  cobol_sort_$RELfb,
	  cobol_sort_$RETfb,
	  cobol_sort_$RETbl	)	fixed bin(35) ext,
    (	  cobol_sort_$rp1,
	  cobol_sort_$rp2,
	  cobol_sort_$stat_ptr,
	  cobol_sort_$error_ptr,
	  cobol_sort_$RELp,
	  cobol_sort_$RETp	)	ptr ext;
/* END INCLUDE FILE ...   cobol_SORTM_entries.incl.pl1 */

